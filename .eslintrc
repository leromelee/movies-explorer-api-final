{
  "extends": "airbnb-base",
  "rules": {
    "eol-last": ["error", "never"],
    "no-console": "off",
    "no-useless-escape": "off",
    "no-shadow": "off",
    "quotes": "off",
    "linebreak-style": "off",
    "consistent-return": "off",
    "no-unresolved": "off",
    "func-names": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  }
}